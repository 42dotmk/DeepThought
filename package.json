{
  "name": "deep-thought",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc --build",
    "start": "npm run clean && npm run build && node build/index.js",
    "startNoClean": "npm run build && node build/index.js",
    "dev": "nodemon --exec \"npm run startNoClean\" --watch src --ext ts",
    "dev:clean": "nodemon --exec \"npm run start\" --watch src --ext ts"
  },

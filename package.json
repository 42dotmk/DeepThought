  "license": "ISC",
  "dependencies": {
    "axios": "^1.11.0",
    "discord.js": "^14.22.0",
    "dotenv": "^17.2.1",
    "glob": "^11.0.3",
    "ollama": "^0.5.17",
    "openai": "^5.13.1"
  },
  "description": "A Discord bot for the Base42 hackerspace community that provides calendar integration and server information.",
  "devDependencies": {
    "@types/node": "^24.2.0",
    "nodemon": "^3.1.10",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
